  "identifier": "SB668",
  "title": "AN ACT RELATING TO STATE AFFAIRS AND GOVERNMENT -- STATE-OWNED AND OPERATED PRINTING FACILITIES PREFERENCE ACT",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "summary",
      "abstract": "Requires that any state agency, state institution of higher education, and quasi-public agency that engages in printing more than 50 copies of a contract, publication, brochure, notice, or promotional material to use a state-owned or operated print shop."
    }
