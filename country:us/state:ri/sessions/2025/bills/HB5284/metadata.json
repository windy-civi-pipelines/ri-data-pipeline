  "abstracts": [
    {
      "note": "summary",
      "abstract": "Exempts any individual who is aging out of foster care from the payment of any fee associated with the issuance of a operator's license as well as driver's education class fees. The fee waiver is in effect until the individual reaches 27 years of age."
    }
